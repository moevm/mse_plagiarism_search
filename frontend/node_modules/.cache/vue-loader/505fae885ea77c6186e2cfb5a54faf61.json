ss=\"input-group mb-3\">\n            <input type=\"text\" class=\"form-control\" id=\"basic-url\" aria-describedby=\"basic-addon3\">\n            <div class=\"input-group-append\">\n              <button class=\"btn btn-outline-secondary\" type=\"button\">Кнопка</button>\n            </div>\n          </div>\n\n\n        </b-tab>\n      </b-tabs>\n      </div>\n      <div class=\"col-4\">\n\n        <b-form-group label=\"Search settings:\">\n          <b-form-checkbox\n              v-for=\"option in searchOptions\"\n              v-model=\"selected\"\n              :key=\"option.value\"\n              :value=\"option.value\"\n              name=\"flavour-3a\"\n          >\n            {{ option.text }}\n          </b-form-checkbox>\n        </b-form-group>\n\n        <b-form-group label=\"Methods of search:\">\n          <b-form-checkbox\n              v-for=\"option in methodsOptions\"\n              v-model=\"selected\"\n              :key=\"option.value\"\n              :value=\"option.value\"\n              name=\"flavour-3a\"\n          >\n            {{ option.text }}\n          </b-form-checkbox>\n        </b-form-group>\n\n        <b-form-group label=\"Open source:\">\n          <b-form-checkbox\n              v-for=\"option in openSourceOptions\"\n              v-model=\"selected\"\n              :key=\"option.value\"\n              :value=\"option.value\"\n              name=\"flavour-3a\"\n          >\n            {{ option.text }}\n          </b-form-checkbox>\n        </b-form-group>\n\n        <b-button variant=\"primary\" size=\"auto\">Start searching</b-button>\n      </div>\n    </div>\n    <div>\n      <div class=\"mt-3\">Config selected : {{ file1 ? file1.name : '' }},  {{selected}}</div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Search\",\n  data() {\n    return {\n      file1 : null,\n      file2 : null,\n      selected: [],\n\n      searchOptions: [\n        { text: 'Database search', value: 'database' },\n  